(jbuild_version 1)

(executable (
  (name one_test)
  (modules (one one_test))
))

(alias (
  (name one/runtest)
  (deps (one_test.exe))
  (action (run ${<}))
))

(executable (
  (name two_test)
  (modules (two two_test))
))

(alias (
  (name two/runtest)
  (deps (two_test.exe))
  (action (run ${<}))
))
